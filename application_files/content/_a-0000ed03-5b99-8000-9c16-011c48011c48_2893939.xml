    <rule>
        <name>MMVA_loadDocumentsFromFolder</name>
        <uuid>_a-0000ed03-5b99-8000-9c16-011c48011c48_2893939</uuid>
        <description>load the available documents from folder</description>
        <parentUuid>cdc99331-32be-4cc4-bd08-92476497169f</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>max(
  #"SYSTEM_SYSRULES_forEach"(
    items: folder(ri!folderId, "documentChildren"),
    expression: if(
      and(
        document(fv!item, "extension") = "pdf",
        find(
          document(fv!item, "name"),
          "Automatic reply_ Testing OOO email"
        ) = 0
      ),
      document(fv!item, "id"),
      null
    ),
    
  )
)</definition>
        <namedTypedValue>
            <name>folderId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
